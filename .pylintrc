[MASTER]
extension-pkg-whitelist = _sane
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-continuation,
    global-statement,
    invalid-name,
    invalid-name,
    no-init,
    no-self-use,
    old-style-class,
    too-few-public-methods,
    too-many-arguments,
    too-many-locals,

[BASIC]
no-docstring-rgx = .*

[REPORTS]
reports = no
score = no
msg-template = {path}:{line}: {C}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 120
expected-line-ending-format = LF

